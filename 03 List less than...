a = [1, 1, 3, 4, 6, 7, 3, 9, 99, 45, 23, 22, 12, 2, 4, 6, 9, 10, 12, 13, 3, 4, 6, 7, 8, 9, 10, 4, 4, 4]
b = []
num =input("Kies een getal \n")
for element in a:
    if element < float(num):
        b.append(element)
print("Nu worden alle getalen uit de lijst geprint die lager zijn dan" + num)
print(b)
